{
  "name": "express-api-starter",
  "version": "1.0.0",
  "description": " A basic typescript starter for an express.js API",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "dev": "nodemon --watch src -e ts --exec \"yarn build:js & yarn start\"",
    "lint": "eslint --config .eslintrc --fix src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c"
  },
  "keywords": [],
  "author": "Capure",
  "repository": {
    "type": "git",
    "url": "https://github.com/Capure/express-api-starter-ts.git"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "@babel/runtime": "^7.13.10",
    "argon2": "^0.27.2",
    "concat-stream": "^2.0.0",
    "express-brute": "^1.0.1",
    "express-hcaptcha-ts": "^0.0.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.0.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/concat-stream": "^1.6.0",
    "@types/express-brute": "^1.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node-fetch": "^2.5.10",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rollup": "^2.47.0",
    "typescript": "^4.1.2"
  }
}
